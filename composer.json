{
  "name": "mihailshumilov/conll-parser",
  "type": "library",
  "description": "Conll-U format parser",
  "license": "MIT",
  "authors": [
    {
      "name": "Mihail Shumilov",
      "email": "mschumilow@gmail.com"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "ConllParser\\": "src/ParserConll"
    },
    "classmap": [
      "src/ParserConll/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ConllParser\\": "tests/ParserConll"
    }
  },
  "minimum-stability": "dev",
  "require": {
    "php": "^7.2.0",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3@dev"
  },
  "scripts": {
    "test": [
      "./vendor/bin/phpunit"
    ]
  }
}
